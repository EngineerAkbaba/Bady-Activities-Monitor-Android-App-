<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.health"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.health.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.health.TimerActivity"
            android:label="@string/title_activity_timer" >
        </activity>
        <activity
            android:name="com.example.health.SelectActActivity"
            android:label="@string/title_activity_select_act" >
        </activity>
        <activity
            android:name="com.example.health.PrevActActivity"
            android:label="@string/title_activity_prev_act" >
        </activity>
        <activity
            android:name="com.example.health.TabSelectActivity"
            android:label="@string/title_activity_tab_select" >
        </activity>
    </application>

</manifest>
